<script type="text/javascript">
	// <![CDATA[
	var pCnt = 0; //liczba tabów
	var pTab = new Array();
	var ppPTab = new Array('bxPPogoda', 'bxPPoczta', 'bxPWaluty', 'bxPGielda', 'bxPKalendarium', 'bxPFinansomat', 'bxPHoroskop', 'bxPDocelu', 'bxPPytamy');
	var B = ID('PTabs').getElementsByTagName('a');
	for (var i=0;i<B.length;i++) {
		pTab[pCnt] = B[i].id;
		pCnt++;
		//B.onclick=function(e){ alert(WP.event.getNode(e).id.substr(4,1)); ppShow(WP.event.getNode(e).id.substr(4,1)); return false; if(e.preventDefault) e.preventDefault(); e.returnValue = false; }
	}

	var ppAct = Math.floor(Math.random()*pCnt+1);

	//odpowiada za zaladowanie kodu htmlowego ajaxem 
	//n - nazwa zakladki ktora kliknelismy
	function ppClick(n) {
		if (!ajaxEnabled) {
			return false;
		}
		pAct = n;
		for (var i=1;i<=pCnt;i++){ 
			if (i==pAct) { 
				ID(pTab[i-1]).className='on';
				ID(ppPTab[(i-1)]).style.display='block'; 
				if(ID(ppPTab[n-1]).innerHTML.length<150) WP.ajax.get({
					url: 'http://www.wp.pl/x_profilowany'+(document.location.href.substr(8,1)=='a'?'_'+ppPTab[n-1].substr(3).toLowerCase():'')+'.html',
					params: {wid:ppPTab[n-1].substr(3).toLowerCase()},
					onSuccess: function(x){
						ID(ppPTab[n-1]).innerHTML = x.text;
						var js = ppPTab[n-1]+'JS';
						if($(js))eval($(js).innerHTML);
					},
					onError: function(){
						ID(ppPTab[n-1]).innerHTML = '<div class="error">błąd połączenia</div>';
					},
					onTimeout: function(){
						ID(ppPTab[n-1]).innerHTML = '<div class="timeout">przekroczony czas oczekiwania</div>';
					}
				});
			} else {
				ID(pTab[i-1]).className='';
				ID(ppPTab[(i-1)]).style.display='none'; 
			}
		}
		return false;
	}

	ppClick(ppAct);
	//touch::start
	var touchTS = -1;
	var touchDTS = -1;
	//po najechaniu na nazwe miasta w pogodzie ma pokazac sie div
	function touchOver(e) {
		if(touchTS==-1) touchTS = setTimeout('touchFire()', 250);
		touchDivOver();
	}
	//po zjechaniu z diva z miastami ma po okreslonym czasie zniknac div
	function touchLinkOut(e) {
		if (touchTS>-1) {
			clearTimeout(touchTS);
			touchTS=-1;
		} 	
		touchDivOut();
	}
	//pokazuje diva z miastami
