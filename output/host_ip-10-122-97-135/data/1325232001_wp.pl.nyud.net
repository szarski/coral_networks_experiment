	// ### przewijanie w html ###
	var gloNAct = 1; //aktywny news
	//przewija kolejne zdjecia w glonewsie htmlowym
	function gloHShow(n){
		if (0 == n) {
			if (gloNAct > 1) {
				gloNAct--;
			} else {
				gloNAct = gloNCnt;
			}
		} else if (99 == n) { //prev
			if (gloNAct < gloNCnt) {
				gloNAct++;
			} else {
				gloNAct = 1;
			}
		} else { //wybrana
			if (n > 0 && n <= gloNCnt) {
				gloNAct = n;
			} else {
				gloNAct = 1;
			}
		}
		for (var i = 1; i <= gloNCnt; i++) {
			if (i == gloNAct) {
				WP.$('#gloB' + i).hide();
				WP.$('#gloB' + i).show();
				WP.$('#gloNews' + i).show();
				WP.$('#gloNL' + i).addClass('on');
			} else {
				WP.$('#gloB' + i).hide();
				WP.$('#gloNews' + i).hide();
				WP.$('#gloNL' + i).removeClass('on');
			}
		}
		return false;
	}
	// ### rotacja w HTMLu ###
	var gloRLock = -1;	//co 6 sekund ma sie przelaczyc zdjecie
	function gloROut() {
		if (-1 == gloRLock) {
			gloRLock = setTimeout('gloHShow(99);gloROver();gloROut();', 6500);
		}
	}
	//jak myszka jest na zdjeciu to ma sie nie rotowac
	function gloROver() {
		if (-1 != gloRLock) {
			clearTimeout(gloRLock);
			gloRLock = -1;
		}
	}
	gloROut();

	//pokazuje glonewsa htmlowego
	ID('gloHTML').onmouseover = function() { gloROver() }
	ID('gloHTML').onmouseout = function() { gloROut() }
	ID('gloNews'+gloNAct).style.display='block';
